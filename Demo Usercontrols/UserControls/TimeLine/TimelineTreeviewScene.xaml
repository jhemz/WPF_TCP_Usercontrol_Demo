<UserControl x:Class="Demo_Usercontrols.UserControls.TimeLine.TimelineTreeviewScene"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Demo_Usercontrols.UserControls.TimeLine"
             mc:Ignorable="d" >
    <TreeViewItem x:Name="Main" FontSize="25" Header="Scene 1" Foreground="White" >
       
        <TreeViewItem.ContextMenu>
            <ContextMenu>
                <MenuItem Header="Rename" x:Name="btnRename" Click="btnRename_Click"/>
                <MenuItem Header="Delete" x:Name="btnDelete" Click="btnDelete_Click"/>
            </ContextMenu>
        </TreeViewItem.ContextMenu>
    </TreeViewItem>
   
</UserControl>
